{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"node": true,
	},
	"parserOptions": {
	    "ecmaVersion": 6,
	    "sourceType": "module",
	    "ecmaFeatures": {
	      "jsx": true,
	      "generators": true,
	      "experimentalObjectRestSpread": true
	    }
	},
	"plugins": [
		"react"
	],
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"rules": {
		"no-console": 0,
		"no-fallthrough": 0,
		"no-undef": 0,
		"no-unused-vars": 0
	}
}